{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.forge-calculator.com"
        }
      ],
      "destination": "https://forge-calculator.com/$1",
      "permanent": true
    },
    {
      "source": "/blog-mastering-the-forge.html",
      "destination": "/blog/mastering-the-forge.html",
      "permanent": true
    },
    {
      "source": "/blog-introducing-calculator.html", 
      "destination": "/blog/introducing-calculator.html",
      "permanent": true
    },
    {
      "source": "/npcs",
      "destination": "/wiki/npcs.html",
      "permanent": true
    },
    {
      "source": "/enemies",
      "destination": "/wiki/enemies.html", 
      "permanent": true
    },
    {
      "source": "/introducing-calculator",
      "destination": "/blog/introducing-calculator.html",
      "permanent": true
    },
    {
      "source": "/mastering-the-forge",
      "destination": "/blog/mastering-the-forge.html",
      "permanent": true
    },
    {
      "source": "/best-recipes",
      "destination": "/recipes/best-recipes.html",
      "permanent": true
    },
    {
      "source": "/stone",
      "destination": "/ore/stone.html",
      "permanent": true
    },
    {
      "source": "/mythril",
      "destination": "/ore/mythril.html",
      "permanent": true
    },
    {
      "source": "/runes",
      "destination": "/wiki/runes.html",
      "permanent": true
    },
    {
      "source": "/gamepass",
      "destination": "/wiki/gamepass.html",
      "permanent": true
    },
    {
      "source": "/races",
      "destination": "/wiki/races.html",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "geolocation=(), microphone=(), camera=()"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains"
        }
      ]
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true
}